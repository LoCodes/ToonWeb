<h1> Login Page: </h1>



<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>



    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit 'Login' %>
<% end %> <br><br>

<h4> Or Login With Google </h4>

<%= button_to 'Google', '/auth/google_oauth2' %>