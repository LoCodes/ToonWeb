

<%= render partial: 'layouts/errors', locals: {object: @user}  %>


<%= form_with model: @user do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %> 

    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit "Sign Up" %>
<% end %>


<h2> Or Sign Up With Google </h2>

<%= button_to 'Google', '/auth/google_oauth2' %>